<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:XamlAppForTesting="clr-namespace:XamlAppForTesting">
    <SolidColorBrush x:Key="red" Color="Red" />
    <DataTemplate x:Key="person" DataType="{x:Type XamlAppForTesting:Person}">
        <Button>Hi</Button>
    </DataTemplate>
    <DataTemplate x:Key="employee" DataType="{x:Type XamlAppForTesting:Employee}">
        <Button>Ho</Button>
    </DataTemplate>
    <DataTemplate x:Key="lifeform" DataType="{x:Type XamlAppForTesting:ILifeform}">
        <Button>Hu</Button>
    </DataTemplate>
</ResourceDictionary>